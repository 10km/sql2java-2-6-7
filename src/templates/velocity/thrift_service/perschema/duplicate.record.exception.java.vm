#parse( "header.include.vm" )
#set($servicePkg = $codewriter.getProperty('swift.service.package'))
#if(!$servicePkg)
#set($servicePkg = $schemaPkg)
#end
#set ( $javaClassName = "DuplicateRecordException" )
$codewriter.setCurrentJavaFilename("${servicePkg}", "${javaClassName}.java")
package ${servicePkg};

import com.facebook.swift.codec.ThriftStruct;
import com.facebook.swift.codec.ThriftField;

/**
 * duplicated record exception
 * @author guyadong
 *
 */
@ThriftStruct
public final class DuplicateRecordException extends BaseServiceException {
    private static final long serialVersionUID = 1L;
    private String message;
    
    public DuplicateRecordException() {
    }

    public DuplicateRecordException(String message) {
        super(message);
        this.message = super.getMessage();
    }

    public DuplicateRecordException(Throwable cause) {
        super(cause);
        this.message = super.getMessage();
    }

    public DuplicateRecordException(String message, Throwable cause) {
        super(message, cause);
        this.message = super.getMessage();
    }

    @Override
    @ThriftField(4)
    public String getMessage() {
        return message;
    }
    @ThriftField
    public void setMessage(String message) {
        this.message = message;
    }
}